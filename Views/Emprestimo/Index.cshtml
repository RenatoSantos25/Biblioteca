@model IEnumerable<EmprestimoModel>

    <div class="container">
        <div class="d-flex justify-content-between aligin-itens-center mb-5">
            <h1>Empéstimos</h1>
            <a class="btn btn-success mx-2">Cadastrar novo empréstimo</a>
        </div>
    </div>
<table class="table table-striped table-dark">
        <thead>
           <tr>
            <th scope="col">Recebedor</th>
            <th scope="col">Fornecedor</th>
            <th scope="col">Livro</th>
            <th scope="col">DataRecebedor</th>
           </tr>
        </thead>
        <tbody>
        @if(Model != null )
        {
            @foreach(var emprestimo in Model)
            {
                <tr>
                    <th scope="row">@emprestimo.Recebedor</th>
                    <th>@emprestimo.Fornecedor</th>
                    <th>@emprestimo.LivroEmprestado</th>
                    <th>@emprestimo.DataRebedor</th>
                </tr>
            }
        }
        </tbody>
    </table>
